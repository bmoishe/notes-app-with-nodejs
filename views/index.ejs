<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>MY APP</title>
</head>
<body>
  <h1 id="welcome">Nodes App<h1>

  <form action="/notes" method="POST">
    <input id="enterNote" type="text" placeholder="note" name="quote">
    <button id="createNote" type="submit">Submit</button>
  </form>
  <div class="note" >


  <ul class="quotes">
  <% for(var i=0; i<quotes.length; i++) {%>
    <li id="notes"class="quote">
      <span><%= quotes[i].name %></span>
      <span><%= quotes[i].quote %></span>
    </li>
  <% } %>
  </ul>
  </div>
    <button id="update"> Darth Vadar invades! </button>
  <script src="main.js"></script>
</body>
</html>
